{
  "name": "svelte-stack",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "bun@1.2.13",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "r:preview": "wrangler dev",
    "sync": "svelte-kit sync",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "l:generate": "drizzle-kit generate --config=./drizzle.config.local.ts",
    "l:push": "drizzle-kit push --config=./drizzle.config.local.ts",
    "l:migrate": "drizzle-kit migrate --config=./drizzle.config.local.ts",
    "l:studio": "drizzle-kit studio --config=./drizzle.config.local.ts",
    "r:generate": "drizzle-kit generate --config=./drizzle.config.remote.ts",
    "r:push": "drizzle-kit push --config=./drizzle.config.remote.ts",
    "r:migrate": "drizzle-kit migrate --config=./drizzle.config.remote.ts",
    "r:studio": "drizzle-kit studio --config=./drizzle.config.remote.ts",
    "lint": "eslint --fix .",
    "test": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:pw": "VITE_COVERAGE=true rimraf .nyc_output && playwright test && nyc report && bunx http-server coverage/playwright --open",
    "test:e2e": "VITE_COVERAGE=true rimraf .nyc_output && playwright test && nyc report && playwright show-report"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.9.1",
    "@auth/sveltekit": "^1.9.2",
    "@fontsource/ubuntu": "^5.2.5",
    "@libsql/client": "^0.15.6",
    "@tanstack/svelte-query": "^5.76.0",
    "@trpc/client": "~10.45.0",
    "@trpc/server": "~10.45.0",
    "@unocss/reset": "^66.1.2",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.43.1",
    "lucide-svelte": "^0.511.0",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.0",
    "tailwind-variants": "^1.0.0",
    "trpc-sveltekit": "^3.6.3",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.1",
    "@cloudflare/workers-types": "^4.20250517.0",
    "@iconify-json/ph": "^1.2.2",
    "@iconify-json/solar": "^1.2.2",
    "@lucide/svelte": "^0.511.0",
    "@playwright/test": "^1.52.0",
    "@sveltejs/adapter-cloudflare": "^7.0.3",
    "@sveltejs/kit": "^2.21.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "@unocss/eslint-plugin": "^66.1.2",
    "@unocss/extractor-svelte": "^66.1.2",
    "@unocss/preset-wind3": "^66.1.2",
    "@vitest/coverage-istanbul": "^3.1.3",
    "@vitest/coverage-v8": "3.1.3",
    "@vitest/ui": "^3.1.3",
    "bits-ui": "^1.4.8",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.27.0",
    "eslint-plugin-svelte": "^3.8.1",
    "jsdom": "^26.1.0",
    "nyc": "^17.1.0",
    "puppeteer-to-istanbul": "^1.4.0",
    "rimraf": "^6.0.1",
    "svelte": "^5.30.2",
    "svelte-check": "^4.2.1",
    "typescript": "^5.8.3",
    "unocss": "^66.1.2",
    "unocss-preset-animations": "^1.2.1",
    "unocss-preset-shadcn": "^0.5.0",
    "v8-to-istanbul": "^9.3.0",
    "vite": "^6.3.5",
    "vitest": "^3.1.3",
    "wrangler": "^4.15.2"
  }
}
